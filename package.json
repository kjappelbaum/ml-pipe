{
  "name": "ml-pipe",
  "version": "0.2.0",
  "description": "Orchestrate ML pipelines",
  "main": "./lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "keywords": [],
  "author": "Kevin Maik Jablonka",
  "license": "MIT",
  "files": [
    "src",
    "lib",
    "lib-esm"
  ],
  "scripts": {
    "build": "npm run tsc && cheminfo-build -r MLPipe",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "jest --coverage",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjappelbaum/ml-pipe.git"
  },
  "bugs": {
    "url": "https://github.com/kjappelbaum/ml-pipe/issues"
  },
  "homepage": "https://github.com/kjappelbaum/ml-pipe#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "cheminfo-build": "^1.2.0",
    "eslint": "^8.34.0",
    "eslint-config-cheminfo-typescript": "^11.2.2",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.2.0",
    "@types/node": "^18.13.0",
    "libsvm-js": "^0.2.1",
    "ml-array-median": "^1.1.6",
    "ml-matrix": "^6.10.4",
    "ml-random-forest": "^2.1.0",
    "ml-regression-multivariate-linear": "^2.0.4",
    "seedrandom": "^3.0.5"
  }
}
